extends layout

block post-head
  script(src="https://cdnjs.cloudflare.com/ajax/libs/benchmark/1.0.0/benchmark.js")
  script(src="/js/main.js")

block content
  h1= test.name
    small from MKS19
  h2.status
  button.btn.btn-primary.pull-right(id='button-test-run') Run test
  h3 Tests to run on snippets
  pre.test= test.test
  h3 Snippets
  each version in test.versions
    h4= version.title
    div.row
      div.col-xs-8
        pre.snippet(data-name="#{version.title}")= version.code
      div.col-xs-4
        div.result(id="result-#{version.title}")
